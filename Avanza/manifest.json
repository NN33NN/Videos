{
  "manifest_package": "org.chromium.webapk.test",
  "scope_url": "https://www.avanza.se/",
  "intent_filters": {
    "scope_url_scheme": "https",
    "scope_url_host": "www.avanza.se",
    "scope_url_path_type": "android:pathPrefix",
    "scope_url_path": "/"
  },
	"author": "ANDROID",
	"name": "Avanza Web",
	"short_name": "Avanza Web",
	"description": "Webbrowser Avanza version",
	"orientation": "portrait-primary",
	"start_url": "/Webapk/",
	"scope": "https://www.avanza.se/",
	"display": "fullscreen",
	"background_color": "#ffffff",
	"theme_color": "#1d3557",
	"icons": [
		{
			"src": "/WebAPK/Avanza/icon.png",
			"sizes": "48x48 72x72 96x96 128x128 192x192 256x256 512x512",
			"type": "image/png"
		}
	],
	"manifest_version": 2,
	"version": "0.1",
	"applications": {
		"gecko": {
			"id": "addon@example.com"
		}
	}
}
